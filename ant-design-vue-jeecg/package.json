{
  "name": "vue-antd-jeecg",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production && vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "build:test": "NODE_ENV=production && vue-cli-service build --mode test",
    "electron:build": "IS_ELECTRON=true && vue-cli-service electron:build",
    "build:dev:win32": "IS_ELECTRON=true && vue-cli-service electron:build --mode dev --win --ia32",
    "build:dev:win64": "IS_ELECTRON=true && vue-cli-service electron:build --mode dev --win --x64",
    "build:test:win32": "IS_ELECTRON=true && vue-cli-service electron:build --mode test --win --ia32",
    "build:test:win64": "IS_ELECTRON=true && vue-cli-service electron:build --mode test --win --x64",
    "build:prod:win32": "IS_ELECTRON=true && vue-cli-service electron:build --mode prod --win --ia32",
    "build:prod:win64": "IS_ELECTRON=true && vue-cli-service electron:build --mode prod --win --x64",
    "build:dev:mac": "IS_ELECTRON=true && vue-cli-service electron:build --mode dev --mac",
    "build:test:mac": "IS_ELECTRON=true && vue-cli-service electron:build --mode test --mac",
    "build:prod:mac": "IS_ELECTRON=true && vue-cli-service electron:build --mode prod --mac",
    "electron:serve": "IS_ELECTRON=true && vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "pre": "cnpm install || yarn --registry https://registry.npm.taobao.org || npm install --registry https://registry.npm.taobao.org "
  },
  "appId": "com.qing.yang.erp",
  "main": "background.js",
  "dependencies": {
    "@antv/data-set": "^0.11.4",
    "@jeecg/antd-online-mini": "3.0.0-RC",
    "@tinymce/tinymce-vue": "2.1.0",
    "@toast-ui/editor": "^2.1.2",
    "ant-design-vue": "^1.7.2",
    "axios": "^0.18.0",
    "bignumber.js": "^9.0.2",
    "china-area-data": "^5.0.1",
    "clipboard": "^2.0.4",
    "codemirror": "^5.46.0",
    "cron-parser": "^2.10.0",
    "dayjs": "^1.8.0",
    "dom-align": "1.12.0",
    "enquire.js": "^2.1.6",
    "js-cookie": "^2.2.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "md5": "^2.2.1",
    "nprogress": "^0.2.0",
    "tinymce": "5.4.1",
    "viser-vue": "^2.4.8",
    "vue": "^2.6.10",
    "vue-area-linkage": "^5.1.0",
    "vue-cropper": "^0.5.4",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.7.0",
    "vue-loader": "^15.7.0",
    "vue-ls": "^3.2.0",
    "vue-photo-preview": "^1.1.3",
    "vue-print-nb-jeecg": "^1.0.9",
    "vue-router": "^3.0.1",
    "vue-splitpane": "^1.0.4",
    "vuedraggable": "^2.20.0",
    "vuex": "^3.1.0",
    "vxe-table": "2.9.13",
    "vxe-table-plugin-antd": "1.8.10",
    "xe-utils": "2.4.8"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-import": "^1.13.3",
    "compression-webpack-plugin": "^3.1.0",
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.1.0",
    "html-webpack-plugin": "^4.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "generator-star-spacing": "off",
      "no-mixed-operators": 0,
      "vue/max-attributes-per-line": [
        2,
        {
          "singleline": 5,
          "multiline": {
            "max": 1,
            "allowFirstLine": false
          }
        }
      ],
      "vue/attribute-hyphenation": 0,
      "vue/html-self-closing": 0,
      "vue/component-name-in-template-casing": 0,
      "vue/html-closing-bracket-spacing": 0,
      "vue/singleline-html-element-content-newline": 0,
      "vue/no-unused-components": 0,
      "vue/multiline-html-element-content-newline": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "vue/html-closing-bracket-newline": 0,
      "vue/no-parsing-error": 0,
      "no-tabs": 0,
      "indent": [
        "off",
        2
      ],
      "no-console": 0,
      "space-before-function-paren": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
